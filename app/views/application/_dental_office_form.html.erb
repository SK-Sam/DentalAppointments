<%= form_tag path, method: method do %>
  <%= label_tag :name %>
  <%= text_field_tag :name, params[:name] %></br></br>
  <%= label_tag :street_address %>
  <%= text_field_tag :street_address, params[:street_address] %></br></br>
  <%= label_tag :city %>
  <%= text_field_tag :city, params[:city] %></br></br>
  <%= label_tag :state %>
  <%= text_field_tag :state, params[:state] %></br></br>
  <%= label_tag :zip_code %>
  <%= text_field_tag :zip_code, params[:zip_code] %></br></br>
  <%= label_tag :insurance_type %>
  <%= text_field_tag :insurance_type, params[:insurance_type] %></br></br>
  <%= submit_tag button_text%>
<% end %>