<section class="feedback_flash">
  <% flash.each do |type, msg| %>
    <p><%= msg unless msg != "Successfully added new Dental Office" %></p>
  <% end %>
</section>

<%= link_to "Add Dental Office", "/dental_offices/new" %>
<% @dental_offices.each do |office| %>
  <ul><%= link_to office.name, "/dental_offices/#{office.id}" %></ul>
<% end %>